[Unit]
Description=SearchWing service for ads-b

After=network.target

[Service]
Type=simple
User=searchwing
ExecStart=/usr/local/bin/dump1090 --write-json /srv/http/adsb/data --net --quiet
WorkingDirectory=/home/searchwing
Restart=always
RestartSec=1
StartLimitInterval=0

[Install]
WantedBy=multi-user.target
